{
	"default": {
		"identifier": "default",
		"description": "Default capability for the main window",
		"local": true,
		"windows": ["main"],
		"permissions": [
			"core:default",
			"dialog:default",
			"dialog:allow-open",
			"dialog:allow-confirm",
			"dialog:allow-message",
			"store:default",
			"store:allow-get",
			"store:allow-set",
			"store:allow-save",
			"store:allow-load",
			"shell:default",
			"shell:allow-open"
		]
	}
}
